{
  "manifest_version": 2,
  "name": "filter hootsuite",
  "description": "filtering",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://hootsuite.com/*"],
      "js": ["js/content.js"]
    }
  ],
  "permissions": [
    "storage"
  ],
  "options_page": "html/options.html"
}
